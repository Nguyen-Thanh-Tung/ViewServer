doctype html
html
    head
        meta(charset='UTF-8')
        title AdminLTE 2 | Flot Charts
        // Tell the browser to be responsive to screen width
        meta(content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no', name='viewport')
        // Bootstrap 3.3.4
        link(href='/bootstrap/css/bootstrap.min.css', rel='stylesheet', type='text/css')
        // Font Awesome Icons
        link(href='https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css', rel='stylesheet', type='text/css')
        // Ionicons
        link(href='https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css', rel='stylesheet', type='text/css')
        // Theme style
        link(href='/dist/css/AdminLTE.min.css', rel='stylesheet', type='text/css')
        // AdminLTE Skins. Choose a skin from the css/skins, folder instead of downloading all of them to reduce the load.
        link(href='/dist/css/skins/_all-skins.min.css', rel='stylesheet', type='text/css')
        style(type="text/css").
            #logInfor span{
                margin-right: 10px
            }

        // HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries
        // WARNING: Respond.js doesn't work if you view the page via file://
        //if lt IE 9
          script(src='https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js')
          script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
        script.
    body.skin-blue.sidebar-mini
        .wrapper
            header.main-header
                // Logo
                a.logo(href='#')
                    span.logo-mini
                        b A
                        | LT
                    // logo for regular state and mobile devices
                    span.logo-lg
                        b Admin
                        | LTE
                // Header Navbar: style can be found in header.less
                nav.navbar.navbar-static-top(role='navigation')
                    // Sidebar toggle button
                    a.sidebar-toggle(href='#', data-toggle='offcanvas', role='button')
                        span.sr-only Toggle navigation
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    .navbar-custom-menu
                        ul.nav.navbar-nav
                            li.dropdown.user.user-menu
                                a.dropdown-toggle(href='#', data-toggle='dropdown')
                                    img.user-image(src='/dist/img/luffy.png', alt='User Image')
                                    span.hidden-xs Alexander Pierce
                                ul.dropdown-menu
                                    // User image
                                    li.user-header
                                        img.img-circle(src='/dist/img/luffy.png', alt='User Image')
                                        p
                                            | Alexander Pierce - Web Developer
                                            small Member since Nov. 2012
                                    // Menu Body
                                    li.user-body
                                        .col-xs-4.text-center
                                            a(href='#') Followers
                                        .col-xs-4.text-center
                                            a(href='#') Sales
                                        .col-xs-4.text-center
                                            a(href='#') Friends
                                    // Menu Footer
                                    li.user-footer
                                        .pull-left
                                            a.btn.btn-default.btn-flat(href='#') Profile
                                        .pull-right
                                            a.btn.btn-default.btn-flat(href='#') Sign out
                            // Control Sidebar Toggle Button
                            li
                                a(href='#', data-toggle='control-sidebar')
                                    i.fa.fa-gears
            // Left side column. contains the logo and sidebar
            aside.main-sidebar
                // sidebar: style can be found in sidebar.less
                section.sidebar
                    // Sidebar user panel
                    .user-panel
                        .pull-left.image
                            img.img-circle(src='/dist/img/luffy.png', alt='User Image')
                        .pull-left.info
                            p Admin
                            a(href='#')
                                i.fa.fa-circle.text-success
                                |  Online
                    // search form
                    form.sidebar-form(action='#', method='get')
                        .input-group
                            input.form-control(type='text', name='q', placeholder='Search...')
                            span.input-group-btn
                                button#search-btn.btn.btn-flat(type='submit', name='search')
                                    i.fa.fa-search
                    // sidebar menu: : style can be found in sidebar.less
                    ul.sidebar-menu
                        li.header MAIN NAVIGATION
                        li.treeview.active
                            a(href='#')
                                i.fa.fa-pie-chart
                                span Charts
                                i.fa.fa-angle-left.pull-right
                            ul.treeview-menu
                                li.active
                                    a(href='#')
                                        i.fa.fa-circle-o
                                        |  Server Information
                        li
                            a(href='#')
                                i.fa.fa-calendar
                                span Calendar
                                small.label.pull-right.bg-red 3
                        li.header CONFIGURATIONS
                        li
                            a(href='#')
                                i.fa.fa-circle-o.text-red
                                span Enable/Disable Server
                        li
                            a(href='#')
                                i.fa.fa-circle-o.text-aqua
                                span Others
            // Content Wrapper. Contains page content
            .content-wrapper
                // Content Header (Page header)
                section.content-header
                    h1
                        | Server Information
                        small preview
                    ol.breadcrumb
                        li
                            a(href='#')
                                i.fa.fa-dashboard
                                |  Home
                        li
                            a(href='#') Charts
                        li.active Server Information
                // Main content
                section.content
                    // Log
                    .row
                        .col-xs-9
                            // interactive chart
                            .box.box-primary
                                .box-header.with-border
                                    h3.box-title Log Information
                                    .box-tools.pull-right
                                        div
                                            | EPS:
                                            b
                                                span 12013
                                .box-body
                                    #logInfor(style='height: 200px;background-color: black; overflow-y: scroll;color: white; padding: 10px; font-size: 16px;')
                        .col-xs-3
                            // interactive chart
                            .box.box-primary
                                .box-header.with-border
                                    h3.box-title Server
                                    .box-tools.pull-right
                                .box-body
                                    div(style='height: 200px;')
                                        .form-group
                                            label(for='sel1') Server Tick
                                            select#sel1.form-control(multiple='')
                                                option Server1
                                                option Server2
                                                option Server3
                                                option Server4
                                                option Server5
                                                option Server6
                                                option Server7
                                                option Server8
                                                option Server9
                                                option Server10
                                                option Server11
                                                option Server12
                    .row
                        .col-xs-12
                            // interactive chart
                            .box.box-primary
                                .box-header.with-border
                                    i.fa.fa-bar-chart-o
                                    h3.box-title Interactive Area Chart
                                    .box-tools.pull-right
                                        | Real time
                                        #realtime.btn-group(data-toggle='btn-toggle')
                                            button.btn.btn-default.btn-xs.active(type='button', data-toggle='on') On
                                            button.btn.btn-default.btn-xs(type='button', data-toggle='off') Off
                                .box-body
                                    #interactive(style='height: 300px;')
                    .row
                        .col-md-6
                            // Line chart
                            .box.box-primary
                                .box-header.with-border
                                    i.fa.fa-bar-chart-o
                                    h3.box-title Request/Response
                                    .box-tools.pull-right
                                        button.btn.btn-box-tool(data-widget='collapse')
                                            i.fa.fa-minus
                                        button.btn.btn-box-tool(data-widget='remove')
                                            i.fa.fa-times
                                .box-body
                                    #line-chart(style='height: 300px;')
                        .col-md-6
                            // Bar chart
                            .box.box-primary
                                .box-header.with-border
                                    i.fa.fa-bar-chart-o
                                    h3.box-title Error/All
                                    .box-tools.pull-right
                                        button.btn.btn-box-tool(data-widget='collapse')
                                            i.fa.fa-minus
                                        button.btn.btn-box-tool(data-widget='remove')
                                            i.fa.fa-times
                                .box-body
                                    #area-chart(style='height: 300px;')
            footer.main-footer
                .pull-right.hidden-xs
                    b Version
                    |  2.2.0
                strong
                    | Copyright © 2017
                    a(href='#') Almsaeed Studio
                    | .
                |  All rights reserved.
            // Control Sidebar
            aside.control-sidebar.control-sidebar-dark
                // Create the tabs
                ul.nav.nav-tabs.nav-justified.control-sidebar-tabs
                    li
                        a(href='#control-sidebar-home-tab', data-toggle='tab')
                            i.fa.fa-home
                    li
                        a(href='#control-sidebar-settings-tab', data-toggle='tab')
                            i.fa.fa-gears
                // Tab panes
                .tab-content
                    // Home tab content
                    #control-sidebar-home-tab.tab-pane
                        h3.control-sidebar-heading Recent Activity
                        ul.control-sidebar-menu
                            li
                                a(href='javascript::;')
                                    i.menu-icon.fa.fa-birthday-cake.bg-red
                                    .menu-info
                                        h4.control-sidebar-subheading Langdon's Birthday
                                        p Will be 23 on April 24th
                            li
                                a(href='javascript::;')
                                    i.menu-icon.fa.fa-user.bg-yellow
                                    .menu-info
                                        h4.control-sidebar-subheading Frodo Updated His Profile
                                        p New phone +1(800)555-1234
                            li
                                a(href='javascript::;')
                                    i.menu-icon.fa.fa-envelope-o.bg-light-blue
                                    .menu-info
                                        h4.control-sidebar-subheading Nora Joined Mailing List
                                        p nora@example.com
                            li
                                a(href='javascript::;')
                                    i.menu-icon.fa.fa-file-code-o.bg-green
                                    .menu-info
                                        h4.control-sidebar-subheading Cron Job 254 Executed
                                        p Execution time 5 seconds
                        h3.control-sidebar-heading Tasks Progress
                        ul.control-sidebar-menu
                            li
                                a(href='javascript::;')
                                    h4.control-sidebar-subheading
                                        | Custom Template Design
                                        span.label.label-danger.pull-right 70%
                                    .progress.progress-xxs
                                        .progress-bar.progress-bar-danger(style='width: 70%')
                            li
                                a(href='javascript::;')
                                    h4.control-sidebar-subheading
                                        | Update Resume
                                        span.label.label-success.pull-right 95%
                                    .progress.progress-xxs
                                        .progress-bar.progress-bar-success(style='width: 95%')
                            li
                                a(href='javascript::;')
                                    h4.control-sidebar-subheading
                                        | Laravel Integration
                                        span.label.label-warning.pull-right 50%
                                    .progress.progress-xxs
                                        .progress-bar.progress-bar-warning(style='width: 50%')
                            li
                                a(href='javascript::;')
                                    h4.control-sidebar-subheading
                                        | Back End Framework
                                        span.label.label-primary.pull-right 68%
                                    .progress.progress-xxs
                                        .progress-bar.progress-bar-primary(style='width: 68%')
                    // Stats tab content
                    #control-sidebar-stats-tab.tab-pane Stats Tab Content
                    // Settings tab content
                    #control-sidebar-settings-tab.tab-pane
                        form(method='post')
                            h3.control-sidebar-heading General Settings
                            .form-group
                                label.control-sidebar-subheading
                                    | Report panel usage
                                    input.pull-right(type='checkbox', checked='')
                                p
                                    | Some information about this general settings option
                            .form-group
                                label.control-sidebar-subheading
                                    | Allow mail redirect
                                    input.pull-right(type='checkbox', checked='')
                                p
                                    | Other sets of options are available
                            .form-group
                                label.control-sidebar-subheading
                                    | Expose author name in posts
                                    input.pull-right(type='checkbox', checked='')
                                p
                                    | Allow the user to show his name in blog posts
                            h3.control-sidebar-heading Chat Settings
                            .form-group
                                label.control-sidebar-subheading
                                    | Show me as online
                                    input.pull-right(type='checkbox', checked='')
                            .form-group
                                label.control-sidebar-subheading
                                    | Turn off notifications
                                    input.pull-right(type='checkbox')
                            .form-group
                                label.control-sidebar-subheading
                                    | Delete chat history
                                    a.text-red.pull-right(href='javascript::;')
                                        i.fa.fa-trash-o
            .control-sidebar-bg

        // jQuery 2.1.4
        script(src='/plugins/jQuery/jQuery-2.1.4.min.js', type='text/javascript')
        // Bootstrap 3.3.2 JS
        script(src='/bootstrap/js/bootstrap.min.js', type='text/javascript')
        // FastClick
        script(src='/plugins/fastclick/fastclick.min.js', type='text/javascript')
        // AdminLTE App
        script(src='/dist/js/app.min.js', type='text/javascript')
        // AdminLTE for demo purposes
        script(src='/dist/js/demo.js', type='text/javascript')
        // FLOT CHARTS
        script(src='/plugins/flot/jquery.flot.min.js', type='text/javascript')
        // FLOT RESIZE PLUGIN - allows the chart to redraw when the window is resized
        script(src='/plugins/flot/jquery.flot.resize.min.js', type='text/javascript')
        // FLOT PIE PLUGIN - also used to draw donut charts
        script(src='/plugins/flot/jquery.flot.pie.min.js', type='text/javascript')
        // FLOT CATEGORIES PLUGIN - Used to draw bar charts
        script(src='/plugins/flot/jquery.flot.categories.min.js', type='text/javascript')

        // Page script
        //script(src='/dist/bundle.js', type='text/javascript')
        script(src='/javascripts/index.js', type='text/javascript')

